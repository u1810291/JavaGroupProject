      <Button fx:id="applySearch" layoutX="575.0" layoutY="167.0" mnemonicParsing="false" onAction="#bookViewApply" prefHeight="39.0" prefWidth="232.0" style="-fx-background-color: #339933;" text="apply" textFill="WHITE">
