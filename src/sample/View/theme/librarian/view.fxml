            <CheckBox fx:id="borrowedBooks" mnemonicParsing="false" prefHeight="36.0" prefWidth="117.0" text="borrowed books" />
            <CheckBox fx:id="overdue" lineSpacing="100.0" mnemonicParsing="false" prefHeight="36.0" prefWidth="73.0" text="overdue" />
            <CheckBox fx:id="title" lineSpacing="100.0" mnemonicParsing="false" prefHeight="36.0" prefWidth="51.0" text="title" />
